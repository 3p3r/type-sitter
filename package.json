{
  "name": "type-sitter",
  "version": "0.1.5",
  "description": "Generate Tree-Sitter Grammars From TypeScript Definitions",
  "bin": "./dist/main.js",
  "main": "bindings/node",
  "types": "./types.d.ts",
  "scripts": {
    "dev": "ts-node --transpile-only index.ts",
    "build": "npm run build:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production"
  },
  "keywords": [
    "tree-sitter",
    "typescript",
    "grammar",
    "parser",
    "generator",
    "language"
  ],
  "author": "Sepehr Laal",
  "license": "MIT",
  "homepage": "https://github.com/3p3r/type-sitter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/type-sitter.git"
  },
  "bugs": {
    "url": "https://github.com/3p3r/type-sitter/issues"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "quicktype": "^23.0.63",
    "quicktype-core": "^23.0.63",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "copy-webpack-plugin": "^11.0.0",
    "nan": "^2.17.0",
    "tree-sitter-cli": "0.20.4",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
